<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <servlet>
        <servlet-name>CustomerServlet</servlet-name
        <servlet-class>CustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomerServlet</servlet-name
        <url-pattern>/CustomerRegistration</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ProductServlet</servlet-name
        <servlet-class>ProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProductServlet</servlet-name
        <url-pattern>/ProductRegistration</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RecipeServlet</servlet-name
        <servlet-class>RecipeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RecipeServlet</servlet-name
        <url-pattern>/RecipeRegistration</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StartupServlet</servlet-name
        <servlet-class>StartupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartupServlet</servlet-name
        <url-pattern>/StartupRegistration</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SupplierServlet</servlet-name
        <servlet-class>SupplierServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SupplierServlet</servlet-name
        <url-pattern>/SupplierRegistration</url-pattern>
    </servlet-mapping>

</web-app>
